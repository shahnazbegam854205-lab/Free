{
  "version": 2,
  "builds": [
    {
      "src": "api/send-telegram.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/send-telegram",
      "dest": "/api/send-telegram",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
